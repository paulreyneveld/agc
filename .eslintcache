{ color: 'red' } : undefined}\n                onClick={onImageUpload}\n                {...dragProps}\n                style={buttonStyle}\n                >\n                Click or Drop here\n                </Button>\n                &nbsp;\n                <Button style={buttonStyle} variant=\"outline-primary\" onClick={onImageRemoveAll}>Remove all images</Button>\n                {imageList.map((image, index) => (\n                <div key={index} className=\"image-item\">\n                    <img src={image['data_url']} alt=\"\" width=\"100\" />\n                    <div className=\"image-item__btn-wrapper\">\n                    <Button style={buttonStyle} style={{marginTop: \"0.5em\"}} button variant=\"outline-primary\" onClick={() => onImageUpdate(index)}>Update</Button>\n                    <Button style={buttonStyle} style={{marginTop: \"0.5em\"}} variant=\"outline-primary\" onClick={() => onImageRemove(index)}>Remove</Button>\n                    </div>\n                </div>\n                ))}\n            </div>\n            )}\n        </ImageUploading>\n        <Button\n          style={buttonStyle}\n          variant=\"outline-primary\"\n          type=\"submit\"\n        >\n          Upload\n        </Button>\n        </Form>\n        </Container>\n        </>\n    )\n}\n\n\nexport default ImageUpload","/Users/paulreyneveld/Documents/GitHub/agc/src/reducers/imageReducer.js",[],"/Users/paulreyneveld/Documents/GitHub/agc/src/components/NewFooter.js",["115"],"import React from 'react'\nimport Container from 'react-bootstrap/Container'\nimport Card from 'react-bootstrap/Card'\n\nconst NewFooter  = () => {\n\n    const style = {\n        backgroundColor: '#FFEA8E',\n        height: '4em',\n        position: 'fixed',\n        bottom: 0,\n        width: '100%'\n    }\n    \n    return (\n        <footer>\n        <div style={style}>\n            <Container>\n            Copyright Alien Girl Cult 2021 <br />\n            </Container>\n        </div>\