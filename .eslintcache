 const [user, setUser] = useState(null)\n\n  useEffect(() => {\n    const checkLoggedIn = () => {\n      const loggedUserJSON = window.localStorage.getItem('loggedBlogappUser')\n      if (loggedUserJSON) { \n        const user = JSON.parse(loggedUserJSON)\n        dispatch(initializeUser(user))\n        console.log(user)\n        setUser(user)\n      }\n    }\n    checkLoggedIn()\n  }, [dispatch])\n  \n  const handleLogout = () => {\n    window.localStorage.removeItem('loggedBlogappUser')\n    dispatch(clearUser(null))\n  }\n\n  return (\n    <>\n    <Router>\n      <Navigation\n        handleLogout={handleLogout}\n      />\n        <Switch>\n          <Route path=\"/blog\">\n            <Blog />\n          </Route>\n          <Route exact path=\"/newblog\" render={(props) => <NewBlog {...props} />} />\n          <Route path=\"/updateblog/:id\"  render={(props) => <UpdateBlog {...props} />} />\n          <Route path=\"/about\">\n            <About />\n          </Route>\n          <Route path=\"/gallery\">\n            <Gallery />\n          </Route>\n          <Route path=\"/login\"  render={(props) => <Login {...props} />}/>\n          <Route path=\"/imageupload\" component={ImageUpload} />\n          <ProtectedRoute exact path=\"/panelgallery\" component={PanelGallery} user={user} />\n          {/* <Route path=\"/panelgallery\">\n            {isAuthenticated ? <PanelGallery /> : <Error />}\n          </Route> */}\n          <Route path=\"/error\" component={Error} />\n          <Route path=\"/\">\n            <Home />\n          </Route>\n        </Switch>\n      <NewFooter />\n          \n    </Router>\n    </>\n  )\n}\n\nexport default App","/Users/paulreyneveld/Documents/GitHub/agc/src/components/Navigation.js",[],"/Users/paulreyneveld/Documents/GitHub/agc/src/components/Footer.js",[],"/Users/paulreyneveld/Documents/GitHub/agc/src/components/Gallery.js",["110"],"import React, { useState, useEffect} from 'react'\nimport Container from 'react-bootstrap/Container